{
  "name": "tgds-2020",
  "version": "0.0.1",
  "description": "TGDS 2020",
  "keywords": "",
  "license": "",
  "author": "",
  "scripts": {
    "p:dev": "PARCEL_MAX_CONCURRENT_CALLS=3 parcel serve *.html --port 1234",
    "p:build": "yarn clear && NODE_ENV=development parcel build index.html --no-source-maps",
    "serve": "yarn build && serve dist/",
    "clear": "rm -R dist/* && rm -R .cache/*",
    "update": "npm-check -u && rm package-lock.json ; yarn",
    "s:sass": "sass style/:style/ --no-source-map --charset",
    "s:sass::watch": "sass style/:dist/ --no-source-map --charset --watch"
  },
  "dependencies": {
    "gsap": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@snowpack/plugin-babel": "^1.0.0",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.8",
    "snowpack": "^2.5.2"
  }
}
