{
  "name": "tgds-2020",
  "version": "0.0.1",
  "description": "TGDS 2020",
  "license": "MIT",
  "scripts": {
    "p:dev": "PARCEL_MAX_CONCURRENT_CALLS=3 parcel serve *.html --port 1234",
    "p:build": "yarn clear && NODE_ENV=development parcel build index.html --no-source-maps",
    "serve": "yarn build && serve dist/",
    "clear": "rm -R dist/* && rm -R .cache/*",
    "update": "npm-check -u && rm package-lock.json ; yarn",
    "sass:build": "sass style/:style/ --no-source-map --charset",
    "sass:watch": "sass style/:style/ --no-source-map --charset --watch",
    "e:build": "npx eleventy",
    "e:serve": "npx eleventy --serve",
    "e:debug": "DEBUG=* npx eleventy",
    "start": "npm-run-all --parallel dev:*",
    "dev:eleventy": "ELEVENTY_ENV=development eleventy --serve",
    "dev:parcel": "parcel watch js/*.js --out-dir _site/js",
    "build": "run-s prod:*",
    "prod:eleventy": "ELEVENTY_ENV=production eleventy --output=./build",
    "prod:parcel": "parcel build js/*.js --out-dir build/js"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@snowpack/plugin-babel": "^1.0.0",
    "chalk": "^4.1.0",
    "gsap": "^3.3.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.8"
  }
}
