  "scripts": {
    "dev": "PARCEL_MAX_CONCURRENT_CALLS=3 parcel serve index.html --port 1234",
    "build": "yarn clear && parcel build index.html --no-source-maps",
    "serve": "yarn build && serve dist/",
    "clear": "rm -R dist/* && rm -R .cache/*",
    "update": "npm-check -u && rm package-lock.json ; yarn"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "dependencies": {
    "gsap": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.8"
  }
}
